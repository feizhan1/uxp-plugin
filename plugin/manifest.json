{
  "id": "7d40622e",
  "name": "IC产品上架图片处理助手",
  "version": "1.0.3",
  "main": "index.html",
  "manifestVersion": 5,
  "host": [
    {
      "app": "PS",
      "minVersion": "23.5.0"
    }
  ],
  "requiredPermissions": {
    "allowCodeGenerationFromStrings": true,
    "localFileSystem": "request",
    "launchProcess": {
      "schemes": [
        "https"
      ],
      "extensions": [
        ".svg",
        ".png",
        ".jpg",
        ".jpeg",
        ".gif",
        ".json"
      ]
    },
    "webview": {
      "allow": "yes",
      "domains": [
        "https://*.adobe.com",
        "https://*.google.com",
        "https://jsonplaceholder.typicode.com",
        "https://api.github.com",
        "https://httpbin.org",
        "https://reqres.in",
        "https://dummyjson.com",
        "https://fakestoreapi.com",
        "https://randomuser.me",
        "https://*.example.com",
        "https://*.test.com",
        "https://*.localhost",
        "https://localhost:*",
        "https://*.trusted-domain.com",
        "https://api.example.com",
        "https://*.apifoxmock.com",
        "http://127.0.0.1:4523",
        "https://picsum.photos",
        "https://img.tvcmall.com",
        "https://src.tvcmall.com",
        "https://*.tvcmall.com",
        "https://openapi.sjlpj.cn:5002",
        "https://www.tvcmall.com",
        "https://tvc-site.s3.amazonaws.com",
        "https://sts.sjlpj.cn"
      ]
    },
    "network": {
      "domains": [
        "https://*.adobe.com",
        "https://*.google.com",
        "https://jsonplaceholder.typicode.com",
        "https://api.github.com",
        "https://httpbin.org",
        "https://reqres.in",
        "https://dummyjson.com",
        "https://fakestoreapi.com",
        "https://randomuser.me",
        "https://*.example.com",
        "https://*.test.com",
        "https://*.localhost",
        "https://localhost:*",
        "https://*.trusted-domain.com",
        "https://api.example.com",
        "https://*.apifoxmock.com",
        "http://127.0.0.1:4523",
        "https://picsum.photos",
        "https://img.tvcmall.com",
        "https://src.tvcmall.com",
        "https://*.tvcmall.com",
        "https://openapi.sjlpj.cn:5002",
        "https://www.tvcmall.com",
        "https://tvc-site.s3.amazonaws.com",
        "https://sts.sjlpj.cn"
      ]
    }
  },
  "entrypoints": [
    {
      "type": "command",
      "id": "showAbout",
      "label": {
        "default": "About photoshop plugin"
      },
      "shortcut": {
        "mac": "shift+cmd+A",
        "win": "ctrl+alt+A"
      }
    },
    {
      "type": "panel",
      "id": "todoList",
      "label": {
        "default": "待处理产品图片"
      },
      "minimumSize": {
        "width": 400,
        "height": 700
      },
      "maximumSize": {
        "width": 2000,
        "height": 2000
      },
      "preferredDockedSize": {
        "width": 230,
        "height": 300
      },
      "preferredFloatingSize": {
        "width": 230,
        "height": 300
      },
      "icons": [
        {
          "width": 32,
          "height": 32,
          "path": "icons/icon_D.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "dark",
            "darkest"
          ],
          "species": [
            "generic"
          ]
        },
        {
          "width": 32,
          "height": 32,
          "path": "icons/icon_N.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "lightest",
            "light"
          ],
          "species": [
            "generic"
          ]
        }
      ]
    }
  ],
  "icons": [
    {
      "width": 32,
      "height": 32,
      "path": "icons/icon_D.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "dark",
        "darkest"
      ],
      "species": [
        "generic"
      ]
    },
    {
      "width": 32,
      "height": 32,
      "path": "icons/icon_N.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "lightest",
        "light"
      ],
      "species": [
        "generic"
      ]
    }
  ]
}